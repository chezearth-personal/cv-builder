{
  "name": "CV Builder server",
  "version": "1.2.29",
  "description": "You enter some basic info on a web form and ChatGPT (OpenAI) creates your CV for you.",
  "keywords": [
    "CV",
    "OpenAI",
    "Automate",
    "CV builder",
    "ChatGPT"
  ],
  "author": "Charles Rethman",
  "packageManager": "yarn@4.3.1",
  "main": "app.js",
  "scripts": {
    "start": "ts-node-dev --respawn --debug --rs --transpile-only --exit-child src/app.ts",
    "build": "tsc -p .",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "rm -rf build && yarn build && yarn typeorm migration:generate -d ./src/utils/data-source.ts ./src/migrations/added-user-entity",
    "db:push": "rm -rf build && yarn build && yarn typeorm migration:run -d src/utils/data-source.ts",
    "serve": "tsc-watch --onSuccess \"nodemon build/app.js\"",
    "test": "test"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "openai": "^4.33.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.4",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^9.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^16.11.10",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.5.2"
  }
}
